        app:layout_constraintStart_toEndOf="@+id/vcard_country_title"
        app:layout_constraintTop_toTopOf="@+id/vcard_country_title" />

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/vcard_zip_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ZIP:"
        android:textSize="12sp"
        android:textStyle="bold"
        android:layout_marginTop="64dp"
        android:layout_marginStart="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/vcard_city_title" />

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/vcard_zip_content"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:ellipsize="end"
        android:textColor="?attr/colorOnSecondary"
        android:maxLines="2"
        android:textSize="22sp"
        android:layout_marginStart="8dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintVertical_bias="0.7"
        app:layout_constraintBottom_toBottomOf="@+id/vcard_zip_title"
        app:layout_constraintStart_toEndOf="@+id/vcard_zip_title"
        app:layout_constraintTop_toTopOf="@+id/vcard_zip_title" />

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/vcard_website_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Website:"
        android:textSize="12sp"
        android:textStyle="bold"
        android:layout_marginTop="64dp"
        android:layout_marginStart="16dp"
        android:layout_marginBottom="32dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/vcard_zip_title"
        app:layout_constraintBottom_toBottomOf="parent"/>

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/vcard_website_content"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:ellipsize="end"
        android:textColor="?attr/colorOnSecondary"
        android:maxLines="2"
        android:textSize="22sp"
        android:layout_marginStart="8dp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintVertical_bias="0.7"
        app:layout_constraintBottom_toBottomOf="@+id/vcard_website_title"
        app:layout_constraintStart_toEndOf="@+id/vcard_website_title"
        app:layout_constraintTop_toTopOf="@+id/vcard_website_title" />

