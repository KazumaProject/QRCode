        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="@+id/vcard_address_title"
        app:layout_constraintStart_toEndOf="@+id/vcard_address_title"
        app:layout_constraintTop_toTopOf="@+id/vcard_address_title" />

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/vcard_city_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="City:"
        android:textSize="12sp"
        android:textStyle="bold"
        android:layout_marginTop="64dp"
        android:layout_marginStart="16dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/vcard_address_title" />

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/vcard_city_content"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ellipsize="end"
        android:textColor="?attr/colorOnSecondary"
        android:maxLines="1"
        android:textSize="22sp"
        android:layout_marginStart="8dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/vcard_city_title"
        app:layout_constraintStart_toEndOf="@+id/vcard_city_title"
        app:layout_constraintTop_toTopOf="@+id/vcard_city_title" />

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/vcard_state_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="64dp"
        android:text="State:"
        android:textSize="12sp"
        android:textStyle="bold"
        android:layout_marginStart="8dp"
        app:layout_constraintStart_toEndOf="@+id/vcard_city_content"
        app:layout_constraintTop_toBottomOf="@+id/vcard_address_title" />

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/vcard_state_content"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ellipsize="end"
        android:textColor="?attr/colorOnSecondary"
        android:maxLines="1"
        android:textSize="22sp"
        android:layout_marginStart="8dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/vcard_state_title"
        app:layout_constraintStart_toEndOf="@+id/vcard_state_title"
        app:layout_constraintTop_toTopOf="@+id/vcard_state_title" />

    <com.google.android.material.textview.MaterialTextView
        android:id="@+id/vcard_country_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="64dp"
        android:text="Country:"
        android:textSize="12sp"
        android:textStyle="bold"
        android:layout_marginStart="8dp"
        app:layout_constraintStart_toEndOf="@+id/vcard_state_content"
